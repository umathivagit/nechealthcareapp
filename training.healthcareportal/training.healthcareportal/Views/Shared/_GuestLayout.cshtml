<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Healthcare Portal - Clinical</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
        .navbar-inverse {
            background-color: #FFDFBF !important;
        }

        .navbar-inverse .navbar-nav > li > a {
            color: black;
        }
        
        .navbar-collapse{
            position: relative !important;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">

        <div class="header">
            <div class="row">
                <div class="col-md-1">
                    <img src="data:text/plain;base64," style="height:58px">
                </div>
                @if (User.Identity.IsAuthenticated)
                {
                    <div class="navbar-collapse collapse position-relative">
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Home", "DoctorPage", "DoctorRegistration")</li>
                            <li>@Html.ActionLink("Today's Appointments","ConfirmedAppointments","Status")</li>
                            <li>@Html.ActionLink("Upcoming Appointments","UpcomingAppointments","Status")</li>
                            @*<li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                            <li>@Html.ActionLink("Logout", "Logout", "Login")</li>
                            <li> <a> Welcome Back! - Dr. @Session["DoctorUser"] </a> </li>
                        </ul>
                    </div>
                }
                else
                {

                    <div class="col-md-2" style="font-size:550px">
                        <strong>
                            @Html.ActionLink("Health Care Portal", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                        </strong>
                    </div>
                    <div class="col-md-6" style="font-size:80px">
                        <span>
                            <strong>
                                @Html.ActionLink("Login", "Index", "Login", new { area = "" }, new { @class = "navbar-brand" })
                            </strong>
                        </span>
                        <span>
                            <strong>
                                @Html.ActionLink("Register", "ChooseAccountType", "User", new { area = "" }, new { @class = "navbar-brand" })
                            </strong>
                        </span>
                    </div>

                }

            </div>
                </div>
</div>
        <div class="container body-content">
            @RenderBody()
            <hr />
        </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
</body>
</html>
